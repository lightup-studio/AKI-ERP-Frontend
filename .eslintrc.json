{
  "extends": ["plugin:@nrwl/nx/react"],
  "root": true,
  "ignorePatterns": ["!**/*"],
  "plugins": ["@nrwl/nx"],
  "rules": {
    "react-hooks/rules-of-hooks": "off"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "extends": ["plugin:@nrwl/nx/typescript"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "extends": ["plugin:@nrwl/nx/javascript"],
      "rules": {}
    }
  ]
}
